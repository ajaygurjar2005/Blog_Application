{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST"]
    }
  ],
  "functions": {
    "api/*": {
      "memory": 512, 
      "maxDuration": 10 
    }
  }
}
