{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js",
      "methods": ["GET", "POST"] // Include other methods your API supports
    }
  ],
  "functions": {
    "api/*": {
      "memory": 512, // Example memory configuration
      "maxDuration": 10 // Example max duration in seconds
    }
  }
}
